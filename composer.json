{
  "name": "lushdigital/microservice-remote-models",
  "description": "A Lumen package to provide a familiar model paradigm for distributed data.",
  "keywords": ["framework", "laravel", "lumen", "microservice", "grpc", "models"],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Dan Richards",
      "email": "dan.richards@lush.co.uk"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "laravel/lumen-framework": "5.3.*",
    "guzzlehttp/promises": "^1.3",
    "guzzlehttp/guzzle": "^6.2",
    "grpc/grpc": "^1.2",
    "lushdigital/microservice-model-utils": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "apigen/apigen": "4.2.x-dev"
  },
  "autoload": {
    "psr-4": {
      "LushDigital\\MicroServiceRemoteModels\\": "src/",
      "": "protobuf/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "minimum-stability": "dev"
}
